{
  "name": "tailtribe-marketplace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env CHOKIDAR_USEPOLLING=1 WATCHPACK_POLLING=true next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:advanced": "tsx prisma/seed-advanced.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:backup:list": "tsx scripts/backup-database.ts list",
    "db:backup:restore": "tsx scripts/backup-database.ts restore",
    "test:caregivers": "tsx scripts/create-test-caregivers.ts",
    "production:check": "tsx scripts/production-check.ts",
    "production:build": "npm run build",
    "load:test": "tsx scripts/load-test.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:flows": "tsx tests/test-all-flows.ts",
    "verify": "tsx tests/test-all-flows.ts",
    "postbuild": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@prisma/client": "6.18.0",
    "@radix-ui/react-accordion": "^1.2.1",
